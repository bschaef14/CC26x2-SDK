<?xml version="1.0" encoding="UTF-8"?>
<!-- IAR Project Connection File -->

<iarProjectConnection version="1.2" name="RF EasyLink Echo (RX, Packet Re-Trasnmitter)" oneShot="true"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="IarProjectConnection_1.2.xsd">

    <device>
        <name>CC1352P1F3</name>
    </device>

    <asmIncludePath>
        <path>$PROJ_DIR$</path>
    </asmIncludePath>

    <includePath>
        <path>$PROJ_DIR$</path>
        <path>$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/source</path>
        <path>$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/source/ti/posix/iar</path>
    </includePath>

    <defines>
        <define>DeviceFamily_CC13X2</define>
    </defines>

    <extraOptions>
        <args>
            <arg>-f$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/kernel/tirtos/builds/cc13x2_cc26x2/release/iar/compiler.opt.defs</arg>
            <arg>--debug</arg>
            <arg>--silent</arg>
            <arg>-e</arg>
            <arg>--aeabi</arg>
            <arg>--thumb</arg>
            <arg>--diag_suppress=Pa050</arg>
        </args>
    </extraOptions>

    <linkerFile>
        <override>true</override>
        <path>$PROJ_DIR$/cc13x2_cc26x2_tirtos.icf</path>
    </linkerFile>

    <linkerExtraOptions>
        <args>
            <arg>-L$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/source</arg>
            <arg>-L$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/kernel/tirtos/packages</arg>
            <arg>-f$PROJ_DIR$/ti_utils_build_linker.cmd.genlibs</arg>
            <arg>-f$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/kernel/tirtos/builds/cc13x2_cc26x2/release/iar/linker.cmd</arg>
            <arg>$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/source/ti/devices/cc13x2_cc26x2/driverlib/bin/iar/driverlib.lib</arg>
            <arg>--silent</arg>
            <arg>--cpu=Cortex-M4F</arg>
        </args>
    </linkerExtraOptions>

    <customBuildTools>
        <customBuildTool atRoot="true" name="SysConfig"> <!-- Custom build tool for entire project-->
            <fileExtensions>.syscfg</fileExtensions>
            <command>$SYSCONFIG_ROOT$/sysconfig_cli.bat -o $PROJ_DIR$ -s "$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/.metadata/product.json" --compiler iar $FILE_PATH$
            </command>
            <output>
                <path>SysConfig_Generated_Files.ipcf</path>
            </output>
            <hasPrio>true</hasPrio> <!-- true: run this tool before all other tools-->
        </customBuildTool>
    </customBuildTools>

    <heaps>
        <heap> <!-- If omitted, id="0" -->
            <size>0</size>
        </heap>
        <heap id="1">
            <size>0</size>
        </heap>
    </heaps>
    <stacks>
        <stack> <!-- If omitted, id="0" -->
            <size>512</size>
        </stack>
    </stacks>

    <preBuild>
        <command>iarbuild.exe $COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/kernel/tirtos/builds/cc13x2_cc26x2/release/iar/tirtos_builds_cc13x2_cc26x2_release.ewp Debug -log all</command>
    </preBuild>

    <files>
        <path copyTo="$PROJ_DIR$/README.md">$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/examples/rtos/CC1352P_2_LAUNCHXL/easylink/rfEasyLinkEchoRx/README.md</path>
        <path copyTo="$PROJ_DIR$/EasyLinkEcho_PerfectEcho.png">$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/examples/rtos/CC1352P_2_LAUNCHXL/easylink/rfEasyLinkEchoRx/EasyLinkEcho_PerfectEcho.png</path>
        <path copyTo="$PROJ_DIR$/EasyLinkEcho_MissedFirstPacket.png">$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/examples/rtos/CC1352P_2_LAUNCHXL/easylink/rfEasyLinkEchoRx/EasyLinkEcho_MissedFirstPacket.png</path>
        <path copyTo="$PROJ_DIR$/EasyLinkEcho_MissingFirstCouplePackets.png">$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/examples/rtos/CC1352P_2_LAUNCHXL/easylink/rfEasyLinkEchoRx/EasyLinkEcho_MissingFirstCouplePackets.png</path>
        <path copyTo="$PROJ_DIR$/EasyLinkEcho_ErrorTxRx.png">$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/examples/rtos/CC1352P_2_LAUNCHXL/easylink/rfEasyLinkEchoRx/EasyLinkEcho_ErrorTxRx.png</path>
        <path copyTo="$PROJ_DIR$/rfEasyLinkEchoRx.syscfg">$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/examples/rtos/CC1352P_2_LAUNCHXL/easylink/rfEasyLinkEchoRx/tirtos/rfEasyLinkEchoRx.syscfg</path>
        <path copyTo="$PROJ_DIR$/overview.rov.json">$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/examples/rtos/CC1352P_2_LAUNCHXL/easylink/rfEasyLinkEchoRx/overview.rov.json</path>
        <path copyTo="$PROJ_DIR$/rfEasyLinkEchoRx.c">$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/examples/rtos/CC1352P_2_LAUNCHXL/easylink/rfEasyLinkEchoRx/rfEasyLinkEchoRx.c</path>
        <path reference="$PROJ_DIR$/Board.html">$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/source/ti/boards/CC1352P_2_LAUNCHXL/Board.html</path>
        <path copyTo="$PROJ_DIR$/cc13x2_cc26x2_tirtos.icf">$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/examples/rtos/CC1352P_2_LAUNCHXL/easylink/rfEasyLinkEchoRx/tirtos/iar/cc13x2_cc26x2_tirtos.icf</path>
        <group name="easylink" build="true">
            <path copyTo="$PROJ_DIR$/easylink/EasyLink.h">$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/examples/rtos/CC1352P_2_LAUNCHXL/easylink/rfEasyLinkEchoRx/easylink/EasyLink.h</path>
            <path copyTo="$PROJ_DIR$/easylink/EasyLink.c">$COM_TI_SIMPLELINK_CC13X2_26X2_SDK_INSTALL_DIR$/examples/rtos/CC1352P_2_LAUNCHXL/easylink/rfEasyLinkEchoRx/easylink/EasyLink.c</path>
        </group>
    </files>
</iarProjectConnection>